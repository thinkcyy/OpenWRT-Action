# 1-1 编译配置
CONFIG_IB=y
CONFIG_IB_STANDALONE=y
CONFIG_SDK=y
CONFIG_NEED_TOOLCHAIN=y
CONFIG_MAKE_TOOLCHAIN=y

# 1-2 busybox组件配置
CONFIG_BUSYBOX_DEFAULT_LOCALE_SUPPORT=y
CONFIG_BUSYBOX_DEFAULT_UNICODE_SUPPORT=y
CONFIG_BUSYBOX_DEFAULT_DMESG=y
CONFIG_BUSYBOX_DEFAULT_DIFF=y
CONFIG_BUSYBOX_DEFAULT_FDISK=y
CONFIG_BUSYBOX_DEFAULT_FDISK_SUPPORT_LARGE_DISKS=y
CONFIG_BUSYBOX_DEFAULT_FEATURE_FDISK_ADVANCED=y
CONFIG_BUSYBOX_DEFAULT_LSOF=y
CONFIG_BUSYBOX_DEFAULT_LSPCI=y
CONFIG_BUSYBOX_DEFAULT_LSUSB=y
CONFIG_BUSYBOX_DEFAULT_LAST=y
CONFIG_BUSYBOX_DEFAULT_TREE=y
CONFIG_BUSYBOX_DEFAULT_WATCH=y
CONFIG_BUSYBOX_DEFAULT_INOTIFYD=y
CONFIG_BUSYBOX_DEFAULT_NETCAT=y
CONFIG_BUSYBOX_DEFAULT_TELNET=y

CONFIG_BUSYBOX_CONFIG_BC=y
# Agustin的24.10仓库中busybox配置bc使用的是单独的包
CONFIG_PACKAGE_bc=y


CONFIG_PACKAGE_inotifywatch=y
CONFIG_PACKAGE_inotifywait=y

# 1-3 Shell组件配置
CONFIG_PACKAGE_pciutils=m
# CONFIG_BUSYBOX_CONFIG_LSUSB is not set
CONFIG_PACKAGE_usbutils=m
# CONFIG_PACKAGE_luci-i18n-base-es is not set
# CONFIG_LUCI_LANG_es is not set
CONFIG_LUCI_LANG_zh_Hans=y
CONFIG_PACKAGE_ss=m
CONFIG_PACKAGE_tc-full=m
CONFIG_PACKAGE_xupnpd=m
CONFIG_PACKAGE_tcpdump=m

# 1-4 OpenWrt组件配置
