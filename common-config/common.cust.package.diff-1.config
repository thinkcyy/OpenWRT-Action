CONFIG_IB=y
CONFIG_IB_STANDALONE=y
CONFIG_SDK=y
CONFIG_NEED_TOOLCHAIN=y
CONFIG_MAKE_TOOLCHAIN=y

CONFIG_PACKAGE_default-settings=y
CONFIG_PACKAGE_default-settings-chn=y
CONFIG_PACKAGE_thinkcy-settings=y

# CONFIG_BUSYBOX_CONFIG_LSPCI is not set
CONFIG_BUSYBOX_CONFIG_BC=y
# Agustin的24.10仓库中busybox配置使用的是CONFIG_BUSYBOX_DEFAULT参数
CONFIG_BUSYBOX_DEFAULT_BC=y
CONFIG_PACKAGE_bc=y
CONFIG_PACKAGE_pciutils=m
# CONFIG_BUSYBOX_CONFIG_LSUSB is not set
CONFIG_PACKAGE_usbutils=m

# 文件监控
